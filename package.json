{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/main.js",
    "build": "babel src --out-dir dist --extensions \".js,.ts\"",
    "start": "node dist/main.js",
    "vercel-build": "npm run build",
    "db:generate": "prisma generate --schema=./prisma/schema/schema.prisma",
    "db:push": "prisma db push --schema=./prisma/schema/schema.prisma",
    "db:pull": "prisma db pull --schema=./prisma/schema/schema.prisma",
    "db:studio": "prisma studio --schema=./prisma/schema/schema.prisma"
  },
  "author": "Rifky firmansyah",
  "license": "ISC",
  "description": "Project server untuk website personal",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.3.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.1",
    "express-rate-limit": "^8.0.0",
    "firebase": "^10.12.5",
    "firebase-admin": "^12.3.0",
    "form-data": "^4.0.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "logger": "^0.0.1",
    "nanoid": "^5.1.5",
    "slugify": "^1.6.6",
    "streamifier": "^0.1.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.15.0"
  },
  "prisma": {
    "schema": "./prisma/schema/schema.prisma"
  }
}
